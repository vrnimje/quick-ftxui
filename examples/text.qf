Vertical {
    bold BlueLight Text("The given button opens chrome in Linux")
    str a
    Red Button {
        "chrome",
        System("/usr/bin/google-chrome-stable"),
        Animated,
        a
    }
    strikethrough Green Text("This text is Green")
    Horizontal {
        hyperlink Blue Text("Google", "https://www.google.com/")
    }
    Button {
        "Exit",
        "Exit"
    }
}